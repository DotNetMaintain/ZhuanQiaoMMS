<?xml version="1.0" encoding="utf-8"?>
<xs:schema id="Rpt_MaterialFinance"
    targetNamespace="http://tempuri.org/Rpt_MaterialFinance.xsd"
    elementFormDefault="qualified"
    xmlns="http://tempuri.org/Rpt_MaterialFinance.xsd"
    xmlns:mstns="http://tempuri.org/Rpt_MaterialFinance.xsd"
    xmlns:xs="http://www.w3.org/2001/XMLSchema"
>

  <xs:element name="MaterialFinanceQuery">
    <xs:complexType>
      <xs:sequence>
       
        <xs:element name="toptype" type="xs:string" />
        <xs:element name="midtype" type="xs:string" />
        <xs:element name="InStoragePrice" type="xs:float" />
        <xs:element name="ReceivePrice" type="xs:float" />
        <xs:element name="LastMonthBalancePrice" type="xs:float" />
        <xs:element name="CurrentMonthBalancePrice" type="xs:float" />

      </xs:sequence>
    </xs:complexType>
  </xs:element>
  
</xs:schema>
